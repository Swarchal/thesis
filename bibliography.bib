journal = {Journal of Cell Biology},
keywords = {Animals,Cattle,Cell Line,Cell Movement,Cell Movement: physiology,Collagen,Collagen: chemistry,Extracellular Matrix,Extracellular Matrix: physiology,Extracellular Matrix: ultrastructure,Humans,Hydrogels,Hydrogels: chemistry,Integrins,Integrins: metabolism,Kinetics,Matrix Metalloproteinases,Matrix Metalloproteinases: metabolism,Matrix Metalloproteinases: physiology,Proteolysis,Rats,Surface Properties},
