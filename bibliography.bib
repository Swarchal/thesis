isbn = {978-1-4577-1350-7},
issn = {{\textless}null{\textgreater}},
journal = {Aistats 2011},
pages = {215--223},
pmid = {43479959},
title = {{An Analysis of Single-Layer Networks in Unsupervised Feature Learning}},
