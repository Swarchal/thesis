pmid = {28771372},
title = {{High-Content Assay Multiplexing for Vascular Toxicity Screening in Induced Pluripotent Stem Cell-Derived Endothelial Cells and Human Umbilical Vein Endothelial Cells}},
url = {http://online.liebertpub.com/doi/10.1089/adt.2017.786},
volume = {15},
year = {2017}
}
@article{Schapiro2017,
