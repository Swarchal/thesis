@article{Jamdade2015,
abstract = {Breast cancer (BC) is the second most common cause of cancer deaths. Triple-negative breast cancer (TNBC) does not show immunohistochemical expression of oestrogen receptors, progesterone receptors or HER2. At present, no suitable treatment option is available for patients with TNBC. This dearth of effective conventional therapies for the treatment of advanced stage breast cancer has provoked the development of novel strategies for the management of patients with TNBC. This review presents recent information associated with different therapeutic options for the treatment of TNBC focusing on promising targets such as the Notch signalling, Wnt/$\beta$-catenin and Hedgehog pathways, in addition to EGFR, PARP1, mTOR, TGF-$\beta$ and angiogenesis inhibitors.},
