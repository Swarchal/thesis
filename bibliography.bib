file = {:home/scott/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Roy, Chatterjee - 1995 - Automated Microscopy and Image Analysis for Androgen Receptor Function.pdf:pdf},
isbn = {978-1-61779-242-7},
issn = {10454403},
journal = {Critical reviews in eukaryotic gene expression},
keywords = {androgen,automated,coregulator,cytometry,endocrine disruptor,fluorescence microscopy,high content analysis,nuclear receptor},
number = {2},
pages = {157--176},
pmid = {8845582},
title = {{Automated Microscopy and Image Analysis for Androgen Receptor Function.}},
