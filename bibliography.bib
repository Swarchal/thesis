journal = {Clinical and Experimental Metastasis},
keywords = {Cancer,Cell compression,ECM,Invadopod,Invasion,Lamellipod,Lateral spike,Microtrack,Migration,Proteases,Proteolysis,Pseudopod},
month = {jan},
number = {4},
pages = {289--298},
pmid = {18600304},
title = {{Mapping proteolytic cancer cell-extracellular matrix interfaces}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18600304},
volume = {26},
year = {2009}
}
