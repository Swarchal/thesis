number = {10},
pages = {1072--1076},
pmid = {29057053},
title = {{Serotonin Analogues as Inhibitors of Breast Cancer Cell Growth}},
url = {http://pubs.acs.org/doi/abs/10.1021/acsmedchemlett.7b00282},
