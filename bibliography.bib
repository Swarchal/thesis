doi = {10.4161/rna.7.5.13239},
file = {:home/scott/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Kozak, Csucs - 2010 - Kernelized Z' factor in multiparametric screening technology.pdf:pdf},
