file = {:home/scott/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Ochoa et al. - 2015 - Phenotype-Guided Natural Products Discovery Using Cytological Profiling.pdf:pdf},
isbn = {1520-6025 (Electronic)$\backslash$r0163-3864 (Linking)},
