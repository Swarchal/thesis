@article{Sodek2008,
abstract = {BACKGROUND: The invasive potential of cancer cells is usually assessed in vitro using Matrigel as a surrogate basement membrane. Yet cancer cell interaction with collagen I matrices is critical, particularly for the peritoneal metastatic route undertaken by several cancer types including ovarian. Matrix metalloprotease (MMP) activity is important to enable cells to overcome the barrier constraints imposed by basement membranes and stromal matrices in vivo. Our objective was to compare matrices reconstituted from collagen I and Matrigel as representative barriers for ovarian cancer cell invasion. METHODS: The requirement of MMP activity for ovarian cancer cell penetration of Matrigel and collagen matrices was assessed in 2D transwell and 3D spheroid culture systems. RESULTS: The broad range MMP inhibitor GM6001 completely prevented cell perforation of polymerised collagen I-coated transwell membranes. In contrast, GM6001 decreased ES-2 cell penetration of Matrigel by only approximately 30{\%} and had no effect on HEY cell Matrigel penetration. In 3D culture, ovarian cancer cells grown as spheroids also migrated into surrounding Matrigel matrices despite MMP blockade. In contrast, MMP activity was required for invasion into 3D matrices of collagen I reconstituted from acid-soluble rat-tail collagen I, but not from pepsin-extracted collagen I (Vitrogen/Purecol), which lacks telopeptide regions. CONCLUSION: Matrigel does not form representative barriers to ovarian cancer cells in either 2D or 3D culture systems. Our findings support the use of collagen I rather than Matrigel as a matrix barrier for invasion studies to better approximate critical interactions and events associated with peritoneal metastasis.},
author = {Sodek, Katharine L and Brown, Theodore J and Ringuette, Maurice J},
doi = {10.1186/1471-2407-8-223},
file = {:home/scott/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Sodek, Brown, Ringuette - 2008 - Collagen I but not Matrigel matrices provide an MMP-dependent barrier to ovarian cancer cell penetratio.pdf:pdf},
isbn = {1471-2407},
issn = {1471-2407},
journal = {BMC cancer},
pages = {223},
pmid = {18681958},
title = {{Collagen I but not Matrigel matrices provide an MMP-dependent barrier to ovarian cancer cell penetration.}},
volume = {8},
year = {2008}
}
@article{Chittajallu2015,
annote = {Quantitative cell-cycle profiling in vivo.

Implanted HT-1080 cells into mice. For live detection a HT-1080 clone with stable expression of a DNA morphology marker and fluorescent ubiquitination-based cell-cycle indicator.

- G1 cells express red fluorescence
- S, G2 and mitotic cells are green

Mixed fluorescent cells with non-fluorescent cell lines to decrease fluorescent cell density and increase accuracy.

Nuclei segmentation
-------------------------

- Difficult in 3D intravital images due to high cell density and variability in nuclei size, shape and appearance.

- Their solution couples marker-controlled watersheds with novel supervised hierachical learning based region-merging method.

- Nuclei were segmented without cell-cycle information

1. locally adaptive thresholding algorithm to obtain coarse foreground-background segmentation.
2. Detect seed points in nuclei using 3D extension of a scale-adaptive multiscale Laplacian-of-Gaussian (LoG) filter, and used them as markers for a marker-controlled watershed algorithm to obtain an initial segmentation result.

- Intentionally over-segmented the nuclei, and then corrected this with a supervised hierachical learning based algorithm that finds all pairs of spatially adjacent regions and presents them to a discriminative two-class classification model based on image derived features. The pairs of the classifier recommends for merging are iteratively mergedd in decreased order of merge probability.

Automated cell-cycle state identification
--------------------------------------------------

- Developed a fully automated supervised classification scheme to identify the cell-cycle state of each detected cell on the basis of image-derived features extracted from the cell-cycle reporter.

- Built a 4-class model that associates a given cell with:
- G1
- late G1/early S
- S/G2
- mitosis

- Then built a coarser, more accurate, classification scheme on two-classes:
- interphase (G1 + late G1/early S + S/G2) versus mitosis
- G1 versus S + G2 + mitosis


Quantitative analysis of cancer drug effects in space and time
---------------------------------------------------------------------------

- Used the workflow to track the effects of cancer therapeutics on cell-cycle progression.

- Analysed the single-cell dynamics of paclitaxel treatment

- An overall downward trend in cell density after treatment. Cell-cycle state quantification showed that mitotic cells cells accumulated after drug injection, peaking on day 2. Cell-cycle arrest was much more modest in the 3D assay compared to that of the 2D cultures.

Anti-mitotic cancer drugs have different effects
------------------------------------------------------------},
author = {Chittajallu, Deepak R and Florian, Stefan and Kohler, Rainer H and Iwamoto, Yoshiko and Orth, James D and Weissleder, Ralph and Danuser, Gaudenz and Mitchison, Timothy J},
doi = {10.1038/nmeth.3363},
file = {:home/scott/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Chittajallu et al. - 2015 - In vivo cell-cycle profiling in xenograft tumors by quantitative intravital microscopy.pdf:pdf},
issn = {1548-7091},
journal = {Nature Methods},
number = {April 2014},
pages = {1--14},
publisher = {Nature Publishing Group},
title = {{In vivo cell-cycle profiling in xenograft tumors by quantitative intravital microscopy}},
url = {http://www.nature.com/doifinder/10.1038/nmeth.3363},
year = {2015}
}
