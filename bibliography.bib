file = {:home/scott/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Vinci et al. - 2012 - Advances in establishment and analysis of three-dimensional tumor spheroid-based functional assays for target vali.pdf:pdf},
isbn = {1741-7007 (Electronic) 1741-7007 (Linking)},
