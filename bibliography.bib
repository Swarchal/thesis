file = {:home/scott/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Kim et al. - 2015 - Cell Invasion Dynamics into a Three Dimensional Extracellular Matrix Fibre Network.pdf:pdf},
isbn = {1939-4586 (Electronic)$\backslash$n1059-1524 (Linking)},
issn = {15537358},
journal = {PLoS Computational Biology},
number = {10},
pages = {e1004535},
pmid = {26436883},
primaryClass = {q-bio},
title = {{Cell Invasion Dynamics into a Three Dimensional Extracellular Matrix Fibre Network}},
