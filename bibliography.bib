title = {{Decoding tumour phenotype by noninvasive imaging using a quantitative radiomics approach}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4059926{\&}tool=pmcentrez{\&}rendertype=abstract},
