isbn = {1558-8238 (Electronic)$\backslash$r0021-9738 (Linking)},
issn = {1558-8238},
journal = {Journal of Clinical Investigation},
keywords = {10,1172,2016,Animals,Antineoplastic Agents/therapeutic use,Breast Neoplasms/classification/*drug therapy/*gen,Cell Line,Female,Gene Expression Profiling,Gene Expression Regulation,Heterologous,Humans,Kaplan-Meier Estimate,Mice,Neoplastic,Nude,Phenotype,Signal Transduction/physiology,Transplantation,Tumor,doi,dx,http,jci,jci45014,nloaded from http,org,org on january 12,www},
