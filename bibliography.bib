journal = {Journal of the American Chemical Society},
keywords = {Animals,Cell Line,Cell Proliferation,Cell Proliferation: drug effects,Drug Delivery Systems,Humans,Inhibitory Concentration 50,Molecular Structure,Phenotype,Protein Binding,Small Molecule Libraries,Small Molecule Libraries: pharmacology,Tumor},
month = {jun},
