isbn = {1476-4598 (Electronic)$\backslash$r1476-4598 (Linking)},
issn = {14764598},
journal = {Molecular Cancer},
keywords = {Antiepileptic,Breast cancer,Metastasis,Phenytoin,Voltage-gated Na+channel},
