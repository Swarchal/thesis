journal = {PloS one},
keywords = {Aged,Antineoplastic Combined Chemotherapy Protocols,Antineoplastic Combined Chemotherapy Protocols: th,Breast Neoplasms,Breast Neoplasms: genetics,Breast Neoplasms: metabolism,Cell Proliferation,Female,Gene Expression Regulation, Neoplastic,Genotype,Humans,Middle Aged,Mutation,Nitriles,Nitriles: administration {\&} dosage,Phosphatidylinositol 3-Kinases,Phosphatidylinositol 3-Kinases: genetics,Receptors, Estrogen,Receptors, Estrogen: metabolism,Sirolimus,Sirolimus: administration {\&} dosage,Sirolimus: analogs {\&} derivatives,TOR Serine-Threonine Kinases,TOR Serine-Threonine Kinases: metabolism,Triazoles,Triazoles: administration {\&} dosage},
