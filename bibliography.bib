journal = {Oncotarget},
keywords = {2013,accepted,amd3100,breast cancer,cxcr4,july 10,july 28,july 30,mammospheres,published,received,sdf-1,stem cells},
number = {3},
pages = {599--612},
pmid = {24583601},
