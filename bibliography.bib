issn = {0036-8075},
journal = {Science},
number = {1998},
pages = {1157--1160},
pmid = {22539553},
title = {{Evolutionary Trade-Offs, Pareto Optimality, and the Geometry of Phenotype Space}},
volume = {336},
year = {2012}
}
