issn = {1552454X},
journal = {Journal of Biomolecular Screening},
keywords = {Active learning,High-content screening,Machine learning,Multiparametric analysis,Phenotypic discovery},
number = {5},
pages = {685--695},
pmid = {24643256},
title = {{Active learning strategies for phenotypic profiling of high-content screens}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24643256},
volume = {19},
year = {2014}
}
