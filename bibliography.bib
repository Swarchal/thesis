doi = {10.1083/jcb.201210152},
file = {:home/scott/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Wolf et al. - 2013 - Physical limits of cell migration control by ECM space and nuclear deformation and tuning by proteolysis and tracti.pdf:pdf},
