journal = {BMC bioinformatics},
keywords = {Algorithms,Automation,Bayes Theorem,Cells,Cells: chemistry,Cellular Structures,Cellular Structures: chemistry,Computational Biology,HeLa Cells,Humans,Image Processing, Computer-Assisted,Image Processing, Computer-Assisted: methods,Microscopy, Fluorescence,Models, Biological,Pattern Recognition, Automated,Proteins,Proteins: analysis,Proteins: classification,Reproducibility of Results},
month = {jan},
number = {1},
pages = {90},
pmid = {16504075},
title = {{A graphical model approach to automated classification of protein subcellular location patterns in multi-cell images.}},
url = {http://www.biomedcentral.com/1471-2105/7/90},
volume = {7},
