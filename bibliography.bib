isbn = {9781424441266},
issn = {1945-8452},
journal = {2010 7th IEEE International Symposium on Biomedical Imaging: From Nano to Macro, ISBI 2010 - Proceedings},
keywords = {Cell polarity,Feature extraction,Fluorescence imaging,Image processing,Neutrophils},
