file = {:home/scott/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Danovi et al. - 2013 - A high-content small molecule screen identifies sensitivity of glioblastoma stem cells to inhibition of polo-like.pdf:pdf},
