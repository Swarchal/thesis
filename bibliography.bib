title = {{Recurrent processing during object recognition}},
url = {http://arxiv.org/abs/1512.03385 http://link.springer.com/10.1007/s11042-017-4440-4 http://www.ncbi.nlm.nih.gov/pubmed/23554596 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3612699 http://journal.frontiersin.org/article/10.3389/fpsyg.2013.0},
volume = {4},
