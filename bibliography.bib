pages = {577--587},
pmid = {25033734},
publisher = {Nature Publishing Group},
title = {{The discovery of first-in-class drugs: origins and evolution.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25033734},
volume = {13},
year = {2014}
}
@article{Lee2013,
abstract = {Innovation and new molecular entity production by the pharmaceutical industry has been below expectations. Surprisingly, more first-in-class small-molecule drugs approved by the U.S. Food and Drug Administration (FDA) between 1999 and 2008 were identified by functional phenotypic lead generation strategies reminiscent of pre-genomics pharmacology than contemporary molecular targeted strategies that encompass the vast majority of lead generation efforts. This observation, in conjunction with the difficulty in validating molecular targets for drug discovery, has diminished the impact of the "genomics revolution" and has led to a growing grassroots movement and now broader trend in pharma to reconsider the use of modern physiology-based or phenotypic drug discovery (PDD) strategies. This "From the Guest Editors" column provides an introduction and overview of the two-part special issues of Journal of Biomolecular Screening on PDD. Terminology and the business case for use of PDD are defined. Key issues such as assay performance, chemical optimization, target identification, and challenges to the organization and implementation of PDD are discussed. Possible solutions for these challenges and a new neoclassic vision for PDD that combines phenotypic and functional approaches with technology innovations resulting from the genomics-driven era of target-based drug discovery (TDD) are also described. Finally, an overview of the manuscripts in this special edition is provided.},
author = {Lee, Jonathan a and Berg, Ellen L},
doi = {10.1177/1087057113506118},
file = {:home/scott/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Lee, Berg - 2013 - Neoclassic drug discovery the case for lead generation using phenotypic and functional approaches.pdf:pdf},
issn = {1552-454X},
journal = {Journal of biomolecular screening},
keywords = {Animals,Biological Assay,Drug Discovery,Drug Discovery: methods,Drug Industry,Genomics,Humans,Molecular Targeted Therapy,Phenotype},
month = {dec},
number = {10},
