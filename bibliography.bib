doi = {10.1093/jnci/94.19.1494},
eprint = {NIHMS150003},
file = {:home/scott/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Wang et al. - 2002 - Phenotypic reversion or death of cancer cells by altering signaling pathways in three-dimensional contexts.pdf:pdf},
isbn = {0027-8874 (Print)},
