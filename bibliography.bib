isbn = {1552-454X (Electronic)1087-0571 (Linking)},
issn = {1552-454X},
journal = {Journal of biomolecular screening},
keywords = {decitabine,elisa,fetal hemoglobin,hbf,hu,hydroxyurea,phenotypic screen,sickle cell anemia,$\gamma$ -globin},
