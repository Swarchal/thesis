issn = {15537390},
journal = {PLoS Genetics},
keywords = {Computer Simulation,Genetic Association Studies,Genome-Wide Association Study,Humans,Models, Genetic,Multivariate Analysis,Polymorphism, Single Nucleotide,Software},
month = {jan},
