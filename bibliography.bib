issn = {10959203},
journal = {Science},
number = {6085},
pages = {1157--1160},
pmid = {22539553},
primaryClass = {cs},
title = {{Evolutionary trade-offs, pareto optimality, and the geometry of phenotype space}},
volume = {336},
year = {2012}
}
