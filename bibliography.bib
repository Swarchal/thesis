file = {:home/scott/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Henriet et al. - 2000 - Contact with fibrillar collagen inhibits melanoma cell proliferation by up-regulating p27KIP1.pdf:pdf},
