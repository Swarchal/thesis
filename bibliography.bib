doi = {10.1038/nmeth1032},
file = {:home/scott/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Loo, Wu, Altschuler - 2007 - Image-based multivariate profiling of drug responses from single cells.pdf:pdf},
