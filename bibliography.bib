@article{Nygaard2016,
abstract = {Removal of, or adjustment for, batch effects or center differences is generally required when such effects are present in data. In particular, when preparing microarray gene expression data from multiple cohorts, array platforms, or batches for later analyses, batch effects can have confounding effects, inducing spurious differences between study groups. Many methods and tools exist for removing batch effects from data. However, when study groups are not evenly distributed across batches, actual group differences may induce apparent batch differences, in which case batch adjustments may bias, usually deflate, group differences. Some tools therefore have the option of preserving the difference between study groups, e.g. using a two-way ANOVA model to simultaneously estimate both group and batch effects. Unfortunately, this approach may systematically induce incorrect group differences in downstream analyses when groups are distributed between the batches in an unbalanced manner. The scientific community seems to be largely unaware of how this approach may lead to false discoveries.},
author = {Nygaard, Vegard and R{\o}dland, Einar Andreas and Hovig, Eivind},
doi = {10.1093/biostatistics/kxv027},
file = {:home/scott/Downloads/kxv027.pdf:pdf},
isbn = {1465-4644},
issn = {14684357},
journal = {Biostatistics},
keywords = {Batch effects,Data normalization,Microarrays,Reproducible research.},
number = {1},
pages = {29--39},
pmid = {26272994},
title = {{Methods that remove batch effects while retaining group differences may lead to exaggerated confidence in downstream analyses}},
volume = {17},
year = {2016}
}
@phdthesis{Vaisipour2014,
abstract = {Gene expression microarrays are widely used to better understand the complex$\backslash$r$\backslash$nbiological mechanisms inside cells. One of the main obstacles of applying statistical$\backslash$r$\backslash$nlearning algorithms to microarray data is the large gap between the number of$\backslash$r$\backslash$nfeatures (p) and the number of available instances (n), i.e., the “large p, small n”$\backslash$r$\backslash$nchallenge. This thesis explores two ways to deal with this challenge.$\backslash$r$\backslash$nOne approach is to increase n by combining similarly appropriate microarray$\backslash$r$\backslash$ndata sets together. This is appealing as there are now many publicly available$\backslash$r$\backslash$nmicroarray studies. The main problem of this approach is the batch effect, i.e., the$\backslash$r$\backslash$ninfluence of non-biological factors on expression intensities that can confound the$\backslash$r$\backslash$nbiological signal. As a result, combining gene expression studies without correcting$\backslash$r$\backslash$nfor batch effects may lead to misleading findings.$\backslash$r$\backslash$nThis thesis proposes a novel batch correction algorithm, called batch effect correction$\backslash$r$\backslash$nusing canonical correlation analysis (BECCA), that assumes the batch effect$\backslash$r$\backslash$nis due to additive independent confounding factors and so utilizes canonical$\backslash$r$\backslash$ncorrelation analysis to separate technical bias from the measured biological signal.$\backslash$r$\backslash$nWe compare BECCA to various existing batch correction algorithms using several$\backslash$r$\backslash$nreal-world gene expression studies and find that BECCA has similar performance.$\backslash$r$\backslash$nThe key advantage of utilizing BECCA, compared to other similar performing algorithms,$\backslash$r$\backslash$nis its flexibility, as BECCA allows the user to adjust how much common$\backslash$r$\backslash$nsignal to preserve across the batches and how much batch related signal to remove$\backslash$r$\backslash$nfrom each one by changing the values of BECCA parameters.$\backslash$r$\backslash$nThe second approach to batch correction considers the wisdom of reducing p by$\backslash$r$\backslash$nselecting a subset of genes. Our experiments suggest that some genes in microarray$\backslash$r$\backslash$ndata sets contain very little biological signal, i.e., including only these genes in the$\backslash$r$\backslash$ncalculations makes all specimens highly correlated, regardless of their tissue of origin$\backslash$r$\backslash$nor disease state. It is, therefore, desirable to identify and remove these misleading$\backslash$r$\backslash$ngenes before conducing downstream analysis or batch correction. For this purpose,$\backslash$r$\backslash$nwe propose an efficient algorithm to extend the single-study variance-based gene$\backslash$r$\backslash$nselection method to a multi-study gene selection algorithm. Our empirical results$\backslash$r$\backslash$nshow this feature selection algorithm outperforms other algorithms in reducing the$\backslash$r$\backslash$ndestructive influence of batch effects.},
author = {Vaisipour, Saman},
file = {:home/scott/Downloads/610c33fe29bcc330fe74d5e26286a04cfb44.pdf:pdf},
pages = {1--175},
school = {University of Alberta},
title = {{Detecting, correcting, and preventing the batch effects in multi-site data, with a focus on gene expression Microarrays}},
year = {2014}
}
