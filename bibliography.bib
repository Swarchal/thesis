file = {:home/scott/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Futamura et al. - 2012 - Morphobase, an Encyclopedic Cell Morphology Database, and Its Use for Drug Target Identification.pdf:pdf},
isbn = {1879-1301 (Electronic)$\backslash$r1074-5521 (Linking)},
issn = {10745521},
journal = {Chemistry and Biology},
number = {12},
pages = {1620--1630},
pmid = {23261605},
publisher = {Elsevier Ltd},
title = {{Morphobase, an encyclopedic cell morphology database, and its use for drug target identification}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1074552112004152},
volume = {19},
year = {2012}
}
@article{Johannessen2015,
abstract = {Over the past decade, tremendous progress in high-throughput small molecule-screening methods has facilitated the rapid expansion of phenotype-based data. Parallel advances in genomic characterization methods have complemented these efforts by providing a growing list of annotated cell line features. Together, these developments have paved the way for feature-based identification of novel, exploitable cellular dependencies, subsequently expanding our therapeutic toolkit in cancer and other diseases. Here, we provide an overview of the evolution of phenotypic small-molecule profiling and discuss the most significant and recent profiling and analytical efforts, their impact on the field, and their clinical ramifications. We additionally provide a perspective for future developments in phenotypic profiling efforts guided by genomic science.},
author = {Johannessen, Cory M. and Clemons, Paul A. and Wagner, Bridget K.},
doi = {10.1016/j.tig.2014.11.002},
file = {:home/scott/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Johannessen, Clemons, Wagner - 2015 - Integrating phenotypic small-molecule profiling and human genetics the next phase in drug discover.pdf:pdf},
isbn = {0168-9525 (Print)$\backslash$r0168-9525 (Linking)},
issn = {13624555},
journal = {Trends in Genetics},
keywords = {Cell line profiling,Genotype-phenotype,Small molecule},
